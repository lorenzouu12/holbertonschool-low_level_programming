#include <stdio.h>

/**
 * main - print all the numbers of base 16
 * in lowercase followed by a new line.
 *
 * Return: Always 0
 */
int main(void)
{
char num;
char lettr;

for (num = '0'; num <= '9'; num++)
putchar (num);
}

for (lettr = 'a'; lettr <= 'f'; lettr++)
{
putchar(lettr);

putchar('\n');

return (0);
}
